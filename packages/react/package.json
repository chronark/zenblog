{
  "name": "@zenblog/react",
  "version": "0.0.1",
  "license": "MIT",
  "private": false,
  "description": "Zenblog React Components",
  "main": "src/index.ts",
  "scripts": {
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ],
    "./tailwind": {
      "require": "./src/tailwind.cjs"
    }
  },
  "files": [
    "src"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "peerDependencies": {
    "next": "*",
    "react": "^17.0.0 || ^18.0.0-0",
    "react-dom": "^17.0.0 || ^18.0.0-0"
  },
  "dependencies": {
    "@swc/helpers": "^0.4.12",
    "@tiptap/pm": "^2.1.16",
    "@tiptap/react": "^2.1.16",
    "@tiptap/starter-kit": "^2.1.16",
    "clsx": "^1.1.1"
  },
  "devDependencies": {
    "@types/react": "latest",
    "eslint": "^8.11.0",
    "typescript": "4.8.4"
  }
}
