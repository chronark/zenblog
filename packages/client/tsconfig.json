{
  "files": ["index.ts"],
  "exclude": [""],
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "module": "ES2020",
    "outDir": "dist",
    "rootDir": "./",
    "sourceMap": true,
    "target": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "stripInternal": true,
    "allowSyntheticDefaultImports": true
  }
}
